{
  "name": "analytics-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "indexer": "tsx indexer/src/indexer.ts",
    "stack:up": "docker-compose up -d && sleep 5 && docker-compose exec web npx prisma migrate deploy || docker-compose exec web npx prisma db push",
    "stack:down": "docker-compose down",
    "stack:reindex": "docker-compose exec -T indexer node scripts/reindex.js && docker-compose restart indexer"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.20",
    "@wagmi/connectors": "^7.1.6",
    "@wagmi/core": "^3.3.2",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "viem": "^2.45.1",
    "wagmi": "^3.4.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
